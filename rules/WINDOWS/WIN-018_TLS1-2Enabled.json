  "title": "TLS_1-2_Enabled",
  "description": "Ensure TLS 1.2 is enabled (server)",
  "type": "registry",
  "appliesTo": [
    "Windows"
  ],
  "severity": "high",
  "path": "HKLM:\\SYSTEM\\CurrentControlSet\\Control\\SecurityProviders\\Schannel\\Protocols\\TLS 1.2\\Server",
  "operator": "eq",
